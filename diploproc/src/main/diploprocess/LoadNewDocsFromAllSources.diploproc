def sourceIds = get from: 'http://localhost:8080/diplocore/diplodata/sources', expect: List

sourceIds.each { sourceId ->
    def newDocsIds = post to: "http://localhost:8080/diplocore/rss-new-docs-finder/source/${sourceId}/new-docs", expect: List

    newDocsIds.each { docId ->
        post to: "http://localhost:8080/diplocore/html-doc-loader/doc/${docId}/load"
        output docId: docId
    }
}